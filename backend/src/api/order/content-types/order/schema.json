{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "products": {
      "type": "json",
      "required": true
    },
    "total": {
      "type": "decimal",
      "required": true
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Completed",
        "Send"
      ]
    }
  }
}
